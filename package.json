{
  "name": "taps-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "expo start",
    "ios": "expo run:ios --device",
    "android": "expo run:android --device",
    "build:dev:ios": "bun eas build -e development-simulator -p ios --non-interactive --no-wait",
    "build:dev:and": "bun eas build -e development -p android --non-interactive --no-wait",
    "build:stag:ios": "bun eas build -e staging -p ios --non-interactive --no-wait",
    "build:stag:and": "bun eas build -e staging -p android --non-interactive -no-wait",
    "build:prod:ios": "bun eas build -e appstore -p ios --non-interactive --auto-submit --no-wait",
    "build:prod:and": "bun eas build -e appstore -p android --non-interactive --auto-submit --no-wait",
    "run:ios": "bun eas build:run -e development-simulator -p ios --latest",
    "run:and": "bun eas build:run -e development -p android --latest",
    "clean": "trash ios android 2>/dev/null || true",
    "format": "prettier --experimental-cli --write",
    "format:all": "bun format .",
    "lint:format": "prettier --experimental-cli --cache --check .",
    "prebuild": "expo prebuild",
    "typecheck": "tsc",
    "doctor": "bun x expo-doctor@latest",
    "doctor:fix": "bun expo install --check",
    "eas": "bun x eas-cli@latest"
  },
  "overrides": {
    "lightningcss": "1.30.1"
  },
  "dependencies": {
    "@instantdb/react-native": "^0.22.51",
    "@legendapp/list": "^2.0.15",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.6.4",
    "@rn-primitives/slot": "^1.2.0",
    "@sentry/react-native": "~7.2.0",
    "assert-never": "^1.4.0",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "54.0.23",
    "expo-application": "^7.0.7",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.17",
    "expo-font": "^14.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.14",
    "expo-system-ui": "~6.0.8",
    "jotai": "^2.15.1",
    "lucide-react-native": "^0.553.0",
    "nativewind": "^5.0.0-preview.2",
    "react": "19.1.0",
    "react-nanny": "^2.15.0",
    "react-native": "0.81.5",
    "react-native-css": "3.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-mmkv": "^4.0.0",
    "react-native-nitro-modules": "^0.31.6",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "tiny-invariant": "^1.3.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@expo/config-types": "^54.0.8",
    "@react-native/metro-babel-transformer": "^0.82.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eas-build-cache-provider": "^16.4.2",
    "expo-build-disk-cache": "^0.7.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-native-svg-transformer": "^1.5.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@sentry/cli",
    "react-native-mmkv",
    "react-native-nitro-modules"
  ]
}
